{
	"extends": "../../../tsconfig.colabo.json",
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
		"baseUrl": ".",
		"strict": true,
		"jsx": "preserve",
		"importHelpers": true,
        "moduleResolution": "node",
		"skipLibCheck": true,
        "sourceMap": true,
        "allowJs": true,
		"preserveConstEnums": true,
		"preserveSymlinks": true,
        "esModuleInterop": true,
		"resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "emitDecoratorMetadata": true,
		"checkJs": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "listFiles": false,
        "lib": [
            "es6",
            "dom"
        ]
    },
    "exclude": [
        "**/*d.ts"
    ],
    "compileOnSave": false
}
