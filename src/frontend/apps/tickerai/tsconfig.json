{
	"extends": "./tsconfig.colabo.json",
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"strict": true,
		"jsx": "preserve",
		"importHelpers": true,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"checkJs": true,
		"allowJs": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"preserveSymlinks": true,
		"baseUrl": ".",
		"types": [
			// "webpack-env",
			"jest",
			"node",
		],
		"lib": [
			"esnext",
			"dom",
			"dom.iterable",
			"scripthost"
		]
	},
	"exclude": [
		"public/**/*",
		"**/*.js",
        // "**/*d.ts",
	]
}
