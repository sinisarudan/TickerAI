{
  "name": "backend-support-colabo-space",
  "description": "Support for Backend part of Colabo.Space (puzzles, config, ...)",
  "version": "0.0.5",
  "private": true,
  "scripts": {
    "old_prepare": "colabo puzzles-offer; colabo puzzles-install; colabo puzzles-build",
    "prepare": "colabo puzzles-offer; colabo puzzles-install",
    "clean_full": "rm -r node_modules/; rm package-lock.json; rm -r dist/; rm yarn.lock; rm -r dev_puzzles/*/*/node_modules; rm -r dev_puzzles/*/*/dist; rm -r dev_puzzles/*/*/yarn.lock; rm -r dev_puzzles/*/*/package-lock.json",
    "test": "mocha --watch --watch-extensions ts -r ts-node/register dev_puzzles/**/tests/*.test.ts tests/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cookie-parser": "^1.4.3",
    "deep-assign": "http://colabo.space/data/downloads/deep-assign-2.0.0.tgz",
    "deepmerge": "^4.2.2",
    "express": "^4.16.4",
    "express-resource": "http://colabo.space/data/downloads/express-resource-1.0.0.tgz",
    "mongoose": "^5.10.7",
    "morgan": "^1.9.1",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/chai-string": "^1.4.2",
    "@types/chai-subset": "^1.3.3",
    "@types/chalk": "^2.2.0",
    "@types/deepmerge": "^2.2.0",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "13.13.16",
    "@types/sinon": "^9.0.8",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "chai-subset": "^1.6.0",
    "dtslint": "^2.0.2",
    "ghooks": "^2.0.0",
    "mocha": "^6.2.2",
    "mocha-clearscreen-reporter": "^1.0.1",
    "nyc": "^14.1.1",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "tslint": "~5.9.1",
    "var": "^0.4.0"
